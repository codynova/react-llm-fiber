{
  "name": "@react-llm-fiber/runtime",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./esm/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./dts/index.d.ts",
      "import": "./esm/index.js",
      "default": "./esm/index.js"
    },
    "./*": {
      "types": "./dts/*.d.ts",
      "import": "./esm/*.js",
      "default": "./esm/*.js"
    }
  },
  "devDependencies": {
    "vitest": "^3.2.4"
  }
}

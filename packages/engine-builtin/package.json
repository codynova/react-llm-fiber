{
  "name": "@react-llm-fiber/engine-builtin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./esm/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./dts/index.d.ts",
      "import": "./esm/index.js",
      "default": "./esm/index.js"
    },
    "./*": {
      "types": "./dts/*.d.ts",
      "import": "./esm/*.js",
      "default": "./esm/*.js"
    }
  },
  "dependencies": {
    "@react-llm-fiber/runtime": "workspace:^",
    "react": "^19.1.1",
    "react-reconciler": "0.32.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.1",
    "@types/react-reconciler": "0.32.0",
    "vitest": "^3.2.4"
  }
}
